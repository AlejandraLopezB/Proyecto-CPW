<p id="notice"><%= notice %></p>

<div class="row center-xs">
  <div class="col-xs-8 col-md-6">
    <div class="box text-left"><h1>Páginas Bloqueadas</h1>

      <table>
        <thead>
          <tr>
            <th>Dirección</th>
            <th colspan="3"></th>
          </tr>
        </thead>

        <tbody>
          <% @bloqueadas.each do |bloqueada| %>
            <tr>
              <td><%= link_to bloqueada.url, bloqueada.url%></td>
              <td></td><td></td><td></td><td></td><td></td>
              <td><%= link_to 'Desbloquear', bloqueada, method: :delete, data: { confirm: 'Estas seguro?' } %></td>
            </tr>
          <% end %>
        </tbody>
      </table>

      <br>
    </div>
  </div>
</div>

